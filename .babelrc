{
  "presets": ["react", "es2015"],
  "env": {
    "development": {
      "plugins": [
      ["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",

          "imports": ["react"],

          "locals": ["module"]
        }]
      }],
      // babel-plugin-import
      ["import", { libraryName: "antd", style:true }] // `style: true` 会加载 less 文件
      ]
    }
  }
}